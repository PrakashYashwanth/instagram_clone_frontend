{
	"name": "instagram_clone_frontend",
	"version": "1.0.0",
	"description": "A frontend client for instagram app",
	"main": "index.js",
	"scripts": {
		"start": "webpack serve",
		"clean": "rimraf dist",
		"prebuild": "npm run clean",
		"build": "cross-env NODE_ENV=production webpack",
		"lint": "npx eslint src",
		"lint:fix": "npm run lint -- --fix",
		"prettier": "npx prettier src --check",
		"prettier:fix": "npm run prettier -- --write",
		"format": "npm run prettier:fix && npm run lint:fix",
		"linter": "npm run linter:eslint",
		"linter:eslint": "eslint --fix --ignore-path .gitignore \"**/*.{js,jsx}\"",
		"linter:prettier": "pretty-quick --staged",
		"pretest": "npm run linter && npm run linter:prettier",
		"test": "jest --passWithNoTests",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PrakashYashwanth/instagram_clone_frontend.git"
	},
	"author": "M S S PRAKASH YASHWANTH",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/PrakashYashwanth/instagram_clone_frontend/issues"
	},
	"homepage": "https://github.com/PrakashYashwanth/instagram_clone_frontend#readme",
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.23.2",
		"@babel/eslint-parser": "^7.22.15",
		"@babel/preset-env": "^7.23.2",
		"@babel/preset-react": "^7.22.15",
		"@commitlint/cli": "^18.2.0",
		"@commitlint/config-angular": "^18.1.0",
		"babel-loader": "^9.1.3",
		"clean-webpack-plugin": "^4.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.8.1",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"html-webpack-plugin": "^5.5.3",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"lint-staged": "^15.0.2",
		"mini-css-extract-plugin": "^2.7.6",
		"postcss": "^8.4.31",
		"postcss-loader": "^7.3.3",
		"postcss-preset-env": "^9.3.0",
		"prettier": "^3.0.3",
		"pretty-quick": "^3.1.3",
		"sass": "^1.69.5",
		"sass-loader": "^13.3.2",
		"style-loader": "^3.3.3",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	}
}
